package com.dss;

public class CharPattern37 {

	public static void main(String[] args) {
		char ch ='E';
		for(int i=0;i<=4;i++) {
			for(int j=0;j<=4;j++) {
				if(j==i) {
					ch=ch;
				
		
			
		}
				else ch++;
				System.out.print(ch);
			}
			System.out.println(" ");
		// TODO Auto-generated method stub

	}

	}
}
