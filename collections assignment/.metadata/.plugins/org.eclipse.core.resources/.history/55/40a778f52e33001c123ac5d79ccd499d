package avc.aa;

import java.util.Scanner;

public class PrimeSum {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n=100;
		for(int i=2;i<=n;i++) {
			int count=0;
			for(int j=1;j<n;j++) {
				if(i%j==0) {
					count++;
				}
			}
				if(count==2) {
					System.out.println(i);
					
				}
			}
		}
		
		// TODO Auto-generated method stub

	}

