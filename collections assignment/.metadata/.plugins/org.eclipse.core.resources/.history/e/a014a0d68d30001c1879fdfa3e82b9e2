import java.util.Scanner;

public class ArrY3 {

		public static void main(String[] args) {
			Scanner sc=new Scanner(System.in);
			int n=sc.nextInt();
			int [] arr=new int[n];
			int arr2[]=arr;
			for(int i=0;i<n;i++) {
				System.out.println("enter array  el"+ (i+1));
				arr[i]=sc.nextInt();
			}
		
			for(int i=0;i<n;i++) {
				
				System.out.print( arr[i]+ " " );
			}

			for(int i=n-1;i>=0;i--) {
				
				System.out.print( arr[i]+ " " );
			}
	
}

}
