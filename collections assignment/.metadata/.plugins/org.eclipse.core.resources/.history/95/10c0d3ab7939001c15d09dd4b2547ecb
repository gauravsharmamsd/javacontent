package com.dss;

import java.util.Scanner;

public class CharPattern36 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for(int i=0;i<n;i++) {
			while(i%2==0) {
				System.out.println(i);
			}
		}
		// TODO Auto-generated method stub

	}

}
