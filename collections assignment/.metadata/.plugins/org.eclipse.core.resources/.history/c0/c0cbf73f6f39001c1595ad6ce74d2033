package com.dss;

public class CharPattern37 {

	public static void main(String[] args) {
		int a=69;
		for(int i=0;i<=4;i++) {
			for(int j=0;j<=i;j++) {
			System.out.print((char)(a-j)+" ");
		}
			for(int j=4;j>=0;j--) {
				System.out.print((char)a+" ");
			}
		System.out.println(" ");
	}
		
		}

}