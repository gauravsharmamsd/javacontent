import java.util.Scanner;

public class UI {
	private static Bank b=new Bank();
private static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) {
		showmenu();
		

	}

	private static void showmenu() throws InsufficientAmountException {
		System.out.println("press 1");
		System.out.println("press 1");
		System.out.println("press 1");
		System.out.println("press 1");
		System.out.println("press 1");
		while(true) {
			int x=sc.nextInt();
			switch(x) {
			case 1:
				String username=sc.next();
				int initailAmount=sc.nextInt();
				b.openAccount(username,initailAmount);
			}
		}

		
	}

	

}
