
public class Account {
private String name;
private String city ;
private int id;

public Account( int accid, String name2, String city2) {

	this.id=accid;
	this.city=city2;
	this.name=name2;
}

public String getName() {
	return name;
}
public int setAccno(int Accno) {
return Accno;
}
public void setName(String name) {
	this.name = name;
}
public String getCity() {
	return city;
}
public void setCity(String city) {
	this.city = city;
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public Account(String name, String city, int id) {
	super();
	this.name = name;
	this.city = city;
	this.id = id;

}
@Override
public String toString() {
	return "Account [name=" + name + ", city=" + city + ", id=" + id + "]";
}

}
