
public class Account {
private String name;
private int amount;

public Account(String name, int amount,int AccountNo) {
	super();
	this.name = name;
	this.amount = amount;

}

public Account() {
	// TODO Auto-generated constructor stub
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
}

public int getAmount() {
	return amount;
}

public void setAmount(int amount) {
	this.amount = amount;
}


public int getAccountNo() {
	return amount;
}

public void setAccountNo(int amount) {
	this.amount = amount;
}

@Override
public String toString() {
	return "Account [name=" + name + ", Account=" + amount + "]";
}

}
