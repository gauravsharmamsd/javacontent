package arraylist;
import java.util.Scanner;
public class method {
	
	
	int add(int a,int b) {
	int c=a+b;
	return c;
	}
	public static void main(String[]args) {
		Scanner sc =new Scanner(System.in);
		System.out.println("enter the two num");
		int a=sc.nextInt();
		int b=sc.nextInt();
		method m=new method();
		int addn=m.add(a,b) ;
		System.out.println("sum of two no" +addn);
		}
		
		
	}


