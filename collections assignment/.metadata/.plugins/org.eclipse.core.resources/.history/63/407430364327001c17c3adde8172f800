package com.ss;


public class Width {
	int x;
	float y;
	double z;

	void width(int x) {
		this.x=x;
		System.out.println("width"  +  x);
	}
	void height(float y) {
		this.y=y;
		System.out.println("width"+  y);
	}
	void depth(double z) {
		this.z=z;
		System.out.println("width"+  z);
	}
	

	public static void main(String args[] ){
		Width w=new Width();
		w.width(10);
		w.height(20);
		w.depth(30);
	}
	

}
