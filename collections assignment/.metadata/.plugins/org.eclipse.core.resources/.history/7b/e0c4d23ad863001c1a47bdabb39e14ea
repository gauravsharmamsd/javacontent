
public class One {
	static Employee[]e=new Employee[100];
	static int count;
	static Employee emp= new Employee();
	static Employee emp1= new Employee();
	
public static void main(String args[]) {
	emp.setCity("Gzb");
	emp.setEid(10);
	emp.setEname(null);
	e[count+1]=emp;
	
	e[count+2]=emp1;
	
	System.out.println(emp);
	System.out.println(emp1);
System.out.println("cityName  "+emp.getCity()+"   "+emp.getEid()+"     "+emp.getEname());
}
}
