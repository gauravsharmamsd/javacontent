package com.dss;

import java.util.Scanner;

public class CharPattern36 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int n1=65;
	for(int i=0;i<=n;i++) {
		for(int j=0;j<=n;j++) {
			System.out.println((char) (n1));
		}
		System.out.println();
		
	}
	}
}
