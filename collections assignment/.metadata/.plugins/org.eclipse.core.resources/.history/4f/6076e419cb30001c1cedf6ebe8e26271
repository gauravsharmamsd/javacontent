import java.util.Scanner;

public class Max2 {
	static int max(int arr[]) {
		int max=arr[0];
		for(int i=0;i<5;i++) {
		if(arr[i]>=max) {
			max =arr[i];
		}
		
	}
		return max;
	}
	public static void main(String args[]) {
		Scanner sc =new Scanner (System.in);
		int arr[]=new int[5];
		for(int i=0;i<5;i++) {
			arr[i]=sc.nextInt();
		}
		max(arr);
		System.out.print(max(arr));
	}

}
