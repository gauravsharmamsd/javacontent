import java.util.Scanner;

public class Duplicate {
public static void main(String args[]) {
	Scanner sc = new Scanner(System.in);
	System.out.println("entr the sixe");
	int n=sc.nextInt();
	int arr[]=new int[n];
	for(int i=0;i<n;i++) {
		arr[i]=sc.nextInt();
		
	}
	for(int i=0;i<n;i++) {
	System.out.print(arr[i]);
}
}
}
