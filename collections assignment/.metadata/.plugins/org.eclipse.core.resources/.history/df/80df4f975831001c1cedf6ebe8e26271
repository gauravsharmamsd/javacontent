import java.util.Scanner;

public class Pattern {

	public static void main(String[] args) {
		int ch=65;
		Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
		for(int i=0;i<n;i++) {
			for(int j=0;j<=i;j++) {
				System.out.print((char )(ch+j));
			}
	
			System.out.println();
		}
		
		// TODO Auto-generated method stub

	}

}
